{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "composite": true,
    "target": "ES2022",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "types": ["vite/client", "react", "react-dom"],
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "declaration": true,
    "declarationMap": false
  },
  "include": ["src"]
}
